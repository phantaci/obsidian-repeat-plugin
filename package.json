{
  "name": "obsidian-repeat-plugin",
  "version": "1.0.0",
  "description": "An Obsidian plugin that allows you to periodically review \"repeating\" notes.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint ./src",
    "test": "NODE_ENV=test jest"
  },
  "keywords": [
    "spaced repetition",
    "periodic",
    "repeating",
    "repeat",
    "surface",
    "resurface",
    "flashcards",
    "daily",
    "weekly",
    "monthly",
    "yearly"
  ],
  "author": "Andre Perunicic",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/luxon": "^3.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "eslint": "^8.24.0",
    "jest": "^29.0.3",
    "obsidian": "^0.16.3",
    "obsidian-dataview": "^0.5.46",
    "ts-jest": "^29.0.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "luxon": "^3.0.3"
  }
}
